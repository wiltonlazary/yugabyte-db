{
  "name": "YB-Customer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .jsx,.js src/",
    "prettier": "prettier --write src/redesign/**/*",
    "start": "EXTEND_ESLINT=true react-scripts start",
    "start:prod": "pushstate-server build",
    "cleanup": "rimraf build && rimraf ../src/main/public",
    "fetch-map": "mkdir -p build/static && curl 'http://s3-us-west-2.amazonaws.com/map.yugabyte.com/map/yb_map.tar.gz' | tar -zxv -C build/static",
    "build": "npm run cleanup && REACT_APP_GIT_COMMIT=$(git rev-parse HEAD) react-scripts build && npm run fetch-map",
    "build-and-copy": "npm run build && ncp build ../src/main/public",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "test": "react-scripts test"
  },
  "engines": {
    "node": ">=12.1x.x",
    "npm": ">=6.1x.x"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "ncp": "2.0.0",
    "prettier": "2.1.0",
    "rimraf": "2.7.1",
    "source-map-explorer": "2.5.0"
  },
  "dependencies": {
    "@types/lodash": "4.14.159",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.9.46",
    "@types/react-bootstrap": "0.32.23",
    "@types/react-dom": "16.9.8",
    "@types/react-router": "3.0.24",
    "@types/react-router-dom": "5.1.5",
    "@types/react-select": "3.0.19",
    "ace-builds": "1.4.12",
    "axios": "0.19.2",
    "bootstrap": "3.4.1",
    "clsx": "1.1.1",
    "copy-to-clipboard": "3.3.1",
    "cron-parser": "2.16.3",
    "cron-validator": "1.1.1",
    "cronstrue": "1.94.0",
    "enumify": "1.0.4",
    "formik": "1.5.8",
    "highlight.js": "9.14.2",
    "intl": "1.2.5",
    "js-cookie": "2.2.1",
    "leaflet": "1.0.2",
    "leaflet.markercluster": "1.4.1",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "moment-precise-range-plugin": "1.3.0",
    "node-sass": "4.14.1",
    "plotly.js": "1.54.1",
    "pluralize": "8.0.0",
    "progressbar.js": "1.1.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-ace": "8.0.0",
    "react-bootstrap": "0.33.1",
    "react-bootstrap-multiselect": "2.4.1",
    "react-bootstrap-table": "4.3.1",
    "react-day-picker": "7.4.8",
    "react-dom": "16.13.1",
    "react-dropzone": "3.13.4",
    "react-fa": "4.2.0",
    "react-hook-form": "6.8.4",
    "react-intl": "2.9.0",
